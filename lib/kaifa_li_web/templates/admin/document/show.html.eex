<h2>Show document</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @document.name %>
  </li>

  <li>
    <strong>Keywords:</strong>
    <%= @document.keywords |> Enum.join(", ") %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @document.url %>
  </li>

</ul>

<%= link "Edit", to: admin_document_path(@conn, :edit, @document) %>
<%= link "Back", to: admin_document_path(@conn, :index) %>
